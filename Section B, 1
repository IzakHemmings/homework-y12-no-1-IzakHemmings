Phrase = input("Enter a phrase \n")
Count1 = 0
Count2 = 0
for letter in phrase:
  if letter in "ABCDEFGHIJKLMNOPQRSTUVWXYZ":
      Count1 = Count1 + 1
  if letter in "!Â£$%^&*():;@'<,>.?/#~":
        count2= count2 + 1

print("Capitals: " + str(count1)
print("Characters: " + str(len(Phrase)))
print("Unique: " + str(count2))
